<p id="notice"><%= notice %></p>

<h3>Listando Isapres</h3>

<table>
  <thead>
    <tr>
      <th>Isapre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @isapres.each do |isapre| %>
      <tr>
        <td><%= isapre.isapre %></td>
        <td><%= link_to 'Ver', isapre %></td>
        <td><%= link_to 'Editar', edit_isapre_path(isapre) %></td>
        <td><%= link_to 'Eliminar', isapre, method: :delete, data: { confirm: 'Â¿Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

