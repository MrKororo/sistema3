<h1>Listando Afps</h1>

<table>
  <thead>
    <tr>
      <th>Afp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @afps.each do |afp| %>
      <tr>
        <td><%= afp.afp %></td>
        <td><%= link_to 'Ver', afp %></td>
        <td><%= link_to 'Editar', edit_afp_path(afp) %></td>
        <td><%= link_to 'Eliminar', afp, method: :delete, data: { confirm: 'Â¿Esta seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

